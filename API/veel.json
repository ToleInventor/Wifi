{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "API/*.js", "use": "@vercel/node" }  # Changed to 'API'
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/api/(.*)", "dest": "/API/$1" }     # Both lowercase and uppercase work
  ]
}
